{
  "name": "influx",
  "version": "4.2.0",
  "description": "InfluxDB Client",
  "main": "./lib/index.js",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "tslint src/**.ts && standard ./test/**/*.js",
    "test": "npm run build && mocha -R spec -r test/unit/_setup.js test/unit/*.test.js && npm run lint",
    "integrate": "npm run build && mocha -R spec test/integration/*.test.js && standard",
    "cover": "npm run build && istanbul cover _mocha -- -R spec -r test/unit/_setup.js test/unit/*.test.js ",
    "travis-test": "npm run build && istanbul cover _mocha --report lcovonly -- -R spec -r test/unit/_setup.js test/unit/*.test.js && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/node-influx/node-influx.git"
  },
  "keywords": [
    "influx",
    "influxdb",
    "time",
    "series",
    "client",
    "db"
  ],
  "contributors": [
    "Ben Evans <ben@bensbit.co.uk> (http://bensbit.co.uk)",
    "Connor Peet <connor@peet.io>",
    "Steffen Konerow <steffen@nrg-media.de> (http://www.nrg-media.de)"
  ],
  "standard": {
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "afterEach",
      "before",
      "after",
      "expect",
      "suite",
      "bench"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^6.0.38",
    "chai": "^3.5.0",
    "coveralls": "^2.11.1",
    "istanbul": "^0.4.3",
    "lodash": "^4.16.2",
    "mocha": "^2.4.5",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "standard": "^7.0.1",
    "tslint": "^3.11.0",
    "typescript": "^2.0.0"
  }
}
